{
  "name": "twimagine",
  "version": "1.0.0",
  "description": "Serverless Twitter bot that generates AI-powered images based on user prompts",
  "main": "index.js",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "localstack:start": "docker-compose up -d",
    "localstack:stop": "docker-compose down",
    "localstack:setup": "chmod +x scripts/setup-localstack.sh && ./scripts/setup-localstack.sh",
    "dev:local": "npm run localstack:start && npm run localstack:setup && npm run dev"
  },
  "keywords": [
    "serverless",
    "twitter",
    "ai",
    "image-generation",
    "aws",
    "sst"
  ],
  "author": "gabrielalmir",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "sst": "^3.17.5",
    "typescript": "^5.3.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-sqs": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "aws-lambda": "^1.0.7",
    "crypto-js": "^4.2.0",
    "stripe": "^14.12.0",
    "twitter-api-v2": "^1.15.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
